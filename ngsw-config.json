{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/assets/images/logo.svg",
          "/assets/images/search.svg",
          "/assets/images/home.svg",
          "/assets/images/calendar-add.svg",
          "/assets/images/bars.svg",
          "/assets/images/arrow.svg",
          "/assets/images/Notification_Bell_Calendar.svg"
        ],
        "urls": [
          "https://fonts.googleapis.com/css2?family=Rubik:wght@400;500;700&amp;display=swap",
          "https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500&amp;display=swap",
          "https://fonts.gstatic.com/**",
          "https://fonts.googleapis.com/icon?family=Material+Icons"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**", "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-freshness",
      "urls": ["https://birthday-database.azurewebsites.net/**"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 20,
        "maxAge": "1d",
        "timeout": "5s"
      }
    }
  ]
}
